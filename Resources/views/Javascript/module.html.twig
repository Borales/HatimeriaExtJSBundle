{% extends adminLayout ? "HatimeriaAdminBundle::layout.html.twig":"::base.html.twig" %}

{% block javascripts %}
<script type="text/javascript">
    {% if class %}
    Ext.require("{{ class }}");
    {% endif %}
    Ext.onReady(function() {
        {% if testClass %}
            Ext.require('{{testClass}}', function() {
                {{ testClass}}.testMe();
                });
        {% else %}
            Ext.create("{{ class }}");
        {% endif %}
    });
</script>
{% endblock %}